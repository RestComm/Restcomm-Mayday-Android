<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Agent_grey"
    tools:context=".MainActivity">


    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_alignParentTop="true"
        android:background="@color/Purple">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:src="@drawable/spanlogo1" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="@color/DarkGrey"
            android:orientation="vertical"></LinearLayout>

        <TextView
            android:id="@+id/textView27"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="70dp"
            android:text="@string/available"
            android:textColor="@color/White"
            android:textSize="25sp" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="30dp"
            android:layout_marginTop="30dp"
            android:src="@drawable/available" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/relativeLayout1"
        android:paddingBottom="16dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:paddingTop="16dp">


        <RelativeLayout
            android:id="@+id/relativeLayout3"
            android:layout_width="match_parent"
            android:layout_height="270dp"
            android:background="@drawable/background_agent">

            <ImageView
                android:id="@+id/imageView20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="30dp"
                android:src="@drawable/profile" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/imageView20"
                android:layout_marginStart="54dp"
                android:layout_toEndOf="@+id/imageView20"
                android:text="@string/Tom_Hardy"
                android:textColor="@color/Black"
                android:textSize="30sp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView6"
                android:layout_below="@+id/textView6"
                android:text="@string/Network_Engineer"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Tom_Hardy_landline"
                android:textSize="25sp"
                android:layout_above="@+id/textView9"
                android:layout_alignEnd="@+id/textView7" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/imageView20"
                android:layout_alignStart="@+id/textView8"
                android:text="@string/Tom_Hardy_mobile"
                android:layout_marginTop="5dp"
                android:textSize="25sp" />

            <ImageView
                android:id="@+id/imageView24"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/contact_phone"
                android:layout_alignTop="@+id/textView8"
                android:layout_alignStart="@+id/textView7" />

            <ImageView
                android:id="@+id/imageView25"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/imageView24"
                android:layout_alignTop="@+id/textView9"
                android:src="@drawable/mobile_phone" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Language"
                android:textColor="@color/Black"
                android:textSize="25sp"
                android:layout_alignBottom="@+id/textView14"
                android:layout_alignStart="@+id/imageView20" />

            <LinearLayout
                android:id="@+id/linearLayout6"
                android:layout_width="0.5dp"
                android:layout_height="match_parent"
                android:layout_alignBottom="@+id/textView10"
                android:layout_marginStart="165dp"
                android:layout_marginTop="10dp"
                android:layout_toEndOf="@+id/textView8"
                android:background="@color/DarkGrey"
                android:orientation="horizontal"></LinearLayout>

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@+id/textView7"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="32dp"
                android:text="@string/Total_Calls"
                android:textColor="@color/Black"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/textView11"
                android:layout_marginEnd="72dp"
                android:text="@string/Toatal_Calls_num"
                android:textColor="@color/BlueColor"
                android:textSize="45sp" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/imageView25"
                android:layout_alignEnd="@+id/textView11"
                android:layout_marginEnd="30dp"
                android:text="@string/Rating"
                android:textColor="@color/Black"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/textView13"
                android:layout_below="@+id/imageView20"
                android:layout_marginRight="1dp"
                android:text="@string/Rating_num"
                android:textColor="@color/BlueColor"
                android:textSize="45sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLayout4"
            android:layout_width="match_parent"
            android:layout_height="400dp"
            android:layout_below="@+id/relativeLayout3"
            android:layout_marginTop="10dp"
            android:padding="1dp"
            android:background="@drawable/background_agent">

            <ImageView
                android:id="@+id/imageView26"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="30dp"
                android:src="@drawable/profilepic" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/imageView26"
                android:layout_marginStart="44dp"
                android:layout_toEndOf="@+id/imageView26"
                android:text="@string/Karane_Kane"
                android:textColor="@color/Black"
                android:textSize="30sp" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/location"
                android:textSize="25sp"
                android:layout_below="@+id/imageView28"
                android:layout_alignStart="@+id/textView17"
                android:layout_marginTop="36dp" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView15"
                android:layout_marginStart="14dp"
                android:layout_marginTop="14dp"
                android:layout_toEndOf="@+id/imageView27"
                android:text="@string/number"
                android:textSize="25sp" />

            <ImageView
                android:id="@+id/imageView27"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/imageView28"
                android:layout_alignTop="@+id/textView18"
                android:src="@drawable/location" />

            <ImageView
                android:id="@+id/imageView28"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView15"
                android:layout_alignTop="@+id/textView17"
                android:layout_marginTop="4dp"
                android:src="@drawable/mobile_phone" />

            <RelativeLayout
                android:id="@+id/relativeLayout7"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_below="@+id/textView18"
                android:layout_marginTop="40dp"
                android:background="@color/Light_Brown"
                android:paddingLeft="5dp">

                <TextView
                    android:id="@+id/textView16"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_marginLeft="5dp"
                    android:layout_alignTop="@+id/textView21"
                    android:text="@string/ticket_id"
                    android:textSize="27sp" />

                <TextView
                    android:id="@+id/textView20"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/textView21"
                    android:layout_marginStart="107dp"
                    android:layout_toEndOf="@+id/textView21"
                    android:text="@string/complaint"
                    android:textSize="27sp" />

                <TextView
                    android:id="@+id/textView21"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="97dp"
                    android:layout_toEndOf="@+id/textView16"
                    android:text="@string/product"
                    android:textSize="27sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relativeLayout8"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@+id/relativeLayout7"
                android:background="@color/White">

                <TextView
                    android:id="@+id/textView19"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="25dp"
                    android:text="@string/wifi"
                    android:textSize="22sp" />

                <TextView
                    android:id="@+id/textView23"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="97dp"
                    android:layout_toEndOf="@+id/textView22"
                    android:text="@string/dlink"
                    android:textSize="22sp" />

                <TextView
                    android:id="@+id/textView22"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignTop="@+id/textView23"
                    android:layout_marginStart="16dp"
                    android:text="@string/AM988D"
                    android:textSize="22sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/relativeLayout8"
                android:background="@color/Light_Grey">

                <TextView
                    android:id="@+id/textView24"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignTop="@+id/textView26"
                    android:layout_marginStart="15dp"
                    android:text="@string/AM988D"
                    android:textSize="22sp" />

                <TextView
                    android:id="@+id/textView25"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="25dp"
                    android:text="@string/wifi"
                    android:textSize="22sp" />

                <TextView
                    android:id="@+id/textView26"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/textView25"
                    android:layout_marginStart="97dp"
                    android:layout_toEndOf="@+id/textView24"
                    android:text="@string/dlink"
                    android:textSize="22sp" />
            </RelativeLayout>


        </RelativeLayout>


