<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/relativeLayout"
    android:background="@color/Agent_grey"
    tools:context=".MainActivity"
    >


    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_alignParentTop="true"
        android:background="@color/Purple">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:src="@drawable/spanlogo" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="@color/DarkGrey"
            android:orientation="vertical"></LinearLayout>

        <TextView
            android:id="@+id/textView27"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="70dp"
            android:text="@string/available"
            android:textColor="@color/White"
            android:textSize="17sp" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="30dp"
            android:layout_marginTop="20dp"
            android:src="@drawable/available" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/relativeLayout1"
        android:paddingBottom="5dp"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:paddingTop="5dp">


        <RelativeLayout
            android:id="@+id/relativeLayout3"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:background="@drawable/background_agent">

            <ImageView
                android:id="@+id/imageView20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:src="@drawable/profile1" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/imageView20"
                android:layout_marginStart="18dp"
                android:layout_toEndOf="@+id/imageView20"
                android:text="@string/Tom_Hardy"
                android:textColor="@color/Black"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView6"
                android:layout_below="@+id/textView6"
                android:layout_marginTop="2dp"
                android:text="@string/Network_Engineer"
                android:textSize="10sp" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Tom_Hardy_landline"
                android:textSize="10sp"
                android:layout_below="@+id/textView12"
                android:layout_centerHorizontal="true" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Tom_Hardy_mobile"
                android:textSize="10sp"
                android:layout_below="@+id/imageView20"
                android:layout_alignEnd="@+id/textView7" />

            <ImageView
                android:id="@+id/imageView24"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/contact_phone"
                android:layout_alignBottom="@+id/textView8"
                android:layout_alignStart="@+id/textView7" />

            <ImageView
                android:id="@+id/imageView25"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/mobile_phone"
                android:layout_alignBottom="@+id/textView9"
                android:layout_alignStart="@+id/imageView24" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_alignStart="@+id/imageView20"
                android:layout_marginBottom="10dp"
                android:text="@string/Language"
                android:textColor="@color/Black"
                android:textSize="12sp" />

            <LinearLayout
                android:id="@+id/linearLayout6"
                android:layout_width="0.5dp"
                android:layout_height="match_parent"
                android:layout_alignBottom="@+id/textView10"
                android:layout_marginStart="45dp"
                android:layout_marginTop="10dp"
                android:layout_toEndOf="@+id/textView8"
                android:background="@color/DarkGrey"
                android:orientation="horizontal"></LinearLayout>

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@+id/textView7"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="18dp"
                android:text="@string/Total_Calls"
                android:textColor="@color/Black"
                android:textSize="10sp" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/textView11"
                android:layout_marginEnd="35dp"
                android:text="@string/Toatal_Calls_num"
                android:textColor="@color/BlueColor"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/imageView25"
                android:layout_alignEnd="@+id/textView11"
                android:layout_marginEnd="20dp"
                android:text="@string/Rating"
                android:layout_marginBottom="12dp"
                android:textColor="@color/Black"
                android:textSize="10sp" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/textView13"
                android:layout_below="@+id/imageView20"
                android:layout_marginRight="1dp"
                android:text="@string/Rating_num"
                android:textColor="@color/BlueColor"
                android:textSize="20sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLayout4"
            android:layout_width="match_parent"
            android:padding="1dp"
            android:background="@drawable/background_agent"
            android:layout_below="@+id/relativeLayout3"
            android:layout_marginTop="10dp"
            android:layout_height="150dp">


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="23dp"
                android:background="@color/Medium_brown"
                android:id="@+id/relativeLayout9">

                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:src="@drawable/electronic"
                    android:id="@+id/imageView29"
                    android:layout_centerVertical="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginStart="16dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/electronic"
                    android:textSize="12sp"
                    android:textColor="@color/Black"
                    android:id="@+id/textView28"
                    android:layout_marginStart="21dp"
                    android:layout_alignBottom="@+id/imageView29"
                    android:layout_toEndOf="@+id/imageView29" />


            </RelativeLayout>


            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/relativeLayout9">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/Pale_brown"
                    android:padding="1dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="          "
                        android:id="@+id/textView31" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/resolution"
                        android:layout_marginLeft="10dp"
                        android:textSize="12sp"
                        android:id="@+id/textView32" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/quick"
                        android:layout_marginLeft="20dp"
                        android:textSize="12sp"
                        android:id="@+id/textView29" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/likes"
                        android:layout_marginLeft="30dp"
                        android:textSize="12sp"
                        android:id="@+id/textView30"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/score"
                        android:layout_marginLeft="30dp"
                        android:textSize="12sp"
                        />

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="35dp"
                        android:layout_height="35dp"

                        android:src="@drawable/profile1"
                        android:id="@+id/imageView30" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="15"
                        android:textSize="20sp"
                        android:layout_marginLeft="40dp"
                        android:id="@+id/textView33" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="50dp"
                        android:text="6"
                        android:textSize="20sp"
                        android:id="@+id/textView34" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="8"
                        android:layout_marginLeft="38sp"
                        android:textSize="20sp"
                        android:id="@+id/textView35" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="61"
                        android:textColor="@color/Purple"
                        android:layout_marginLeft="35dp"
                        android:textSize="20sp"
                        android:id="@+id/textView36" />
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:src="@drawable/profile2"
                        android:id="@+id/imageView31" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="12"
                        android:layout_marginLeft="40dp"
                        android:textSize="20sp"
                        android:id="@+id/textView37" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="4"
                        android:layout_marginLeft="50dp"
                        android:textSize="20sp"
                        android:id="@+id/textView38" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="7"
                        android:layout_marginLeft="38sp"
                        android:textSize="20sp"
                        android:id="@+id/textView39" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="42"
                        android:textColor="@color/Orange"
                        android:layout_marginLeft="35dp"
                        android:textSize="20sp"
                        android:id="@+id/textView40" />
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:src="@drawable/profile3"
                        android:id="@+id/imageView32" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="8"
                        android:layout_marginLeft="40dp"
                        android:textSize="20sp"
                        android:id="@+id/textView41" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="4"
                        android:textSize="20sp"
                        android:layout_marginLeft="50dp"
                        android:id="@+id/textView42" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="5"
                        android:layout_marginLeft="38sp"
                        android:textSize="20sp"
                        android:id="@+id/textView43" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="30"
                        android:textColor="@color/Orange"
                        android:layout_marginLeft="35dp"
                        android:textSize="20sp"
                        android:id="@+id/textView44" />
                </TableRow>
            </TableLayout>
        </RelativeLayout>

        <RelativeLayout
            android:layout_marginTop="10dp"
            android:id="@+id/relativeLayout5"
            android:layout_below="@+id/relativeLayout4"
            android:background="@drawable/background_agent"
            android:layout_width="match_parent"
            android:layout_height="120dp">


            <Button
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:textSize="20sp"
                android:text="17"
                android:id="@+id/button"
                android:background="@drawable/shape_button1"
                android:layout_marginStart="26dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="15dp" />

            <Button
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:textSize="20sp"
                android:text="3"
                android:id="@+id/button2"
                android:background="@drawable/shape_button4"
                android:layout_alignParentBottom="true"
                android:layout_alignStart="@+id/button"
                android:layout_marginBottom="8dp" />

            <Button
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:textSize="20sp"
                android:text="12"
                android:background="@drawable/shape_button3"
                android:id="@+id/button3"
                android:layout_alignTop="@+id/button2"
                android:layout_alignStart="@+id/button4" />

            <Button
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:textSize="20sp"
                android:text="5"
                android:id="@+id/button4"
                android:background="@drawable/shape_button2"
                android:layout_marginStart="36dp"
                android:layout_alignBottom="@+id/textView47"
                android:layout_toEndOf="@+id/textView47" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/calls_dropped"
                android:textSize="10sp"
                android:id="@+id/textView45"
                android:layout_alignTop="@+id/textView48"
                android:layout_alignStart="@+id/textView46" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/calls_in_que"
                android:textSize="10sp"
                android:id="@+id/textView46"
                android:layout_marginStart="32dp"
                android:layout_alignBaseline="@+id/textView47"
                android:layout_alignBottom="@+id/textView47"
                android:layout_toEndOf="@+id/button3" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/num_calls"
                android:textSize="10sp"
                android:id="@+id/textView47"
                android:layout_alignBottom="@+id/button"
                android:layout_alignStart="@+id/textView48" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/calls_attended"
                android:textSize="10sp"
                android:id="@+id/textView48"
                android:layout_marginStart="32dp"
                android:layout_alignTop="@+id/button2"
                android:layout_toEndOf="@+id/button2" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/backround_border"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true">


            <RelativeLayout
                android:layout_width="90dp"
                android:background="@color/Pale_purple"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:id="@+id/relativeLayout11">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="16sp"
                    android:text="@string/event"
                    android:layout_marginTop="10dp"
                    android:textColor="@color/Black"
                    android:id="@+id/textView49"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="10dp"
                android:layout_toEndOf="@+id/relativeLayout11">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/server"
                    android:textSize="11sp"
                    android:id="@+id/textView50"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true" />


            </RelativeLayout>


        </RelativeLayout>



    </RelativeLayout>




</RelativeLayout>